{
  "name": "hello-app",
  "version": "0.0.0",
  "scripts": {
    "clean": "rm -rf dist",
    "serve": "http-server . -p 5556",
    "build": "ngc -p tsconfig.json",
    "rollup": "rollup -c",
    "wp_build": "webpack --config webpack.config.js",
    "es5": "tsc --target es5 --allowJs dist/bundle.es2015.js --out dist/bundle.js",
    "minify": "terser dist/bundle.es2015.js --compress --mangle --output dist/bundle.es2015.min.js",
    "build_prod": "npm run clean && npm run build && npm run rollup && npm run minify",
    "rollup_build": "npm run clean && npm run build && npm run rollup",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "rxjs": "~6.5.4",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "9.1.1",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "5.2.1",
    "clean-webpack-plugin": "3.0.0",
    "html-webpack-plugin": "4.2.0",
    "http-server": "^0.12.1",
    "path": "0.12.7",
    "raw-loader": "4.0.1",
    "terser": "4.6.10",
    "terser-webpack-plugin": "^2.3.6",
    "typescript": "~3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
